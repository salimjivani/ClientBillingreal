@{
    ViewBag.Title = "Home Page";
}

<div id="testing">Click me</div>
<div id="codemaint">Code Maint Click here</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script>

    $("#codemaint").click(function () {
        $.ajax({
            url: "/Home/Businesslogicinputs",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            async: false,
            type: "POST",
            data: "data",
            success: function (json) {
                console.log(json);
                $('#codemaint').html(json[3].Titles[7] + " : " + json[3].Details[0].DetailModelValues[7].value);
                
            },
            error: function () {    
                $("#codemaint").html("ERROR");
            }
        });
    });    

</script>